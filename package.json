{
  "name": "lambda-coldstart-analyzer",
  "version": "0.1.0",
  "private": true,
  "description": "Monorepo for Lambda Cold-Start Analyzer (API + Web + Libraries)",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "test:analysis": "node -r ts-node/register libs/analysis/tests/aggregate.test.ts",
    "test:api": "node -r ts-node/register apps/api/tests/run.ts"
  },
  "devDependencies": {
    "turbo": "^2.2.3",
    "typescript": "^5.5.3",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/node": "^20.11.30",
    "bcryptjs": "^2.4.3"
  }
}
